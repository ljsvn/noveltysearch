<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="com.ods.base.bo.menu">
	<class name="MenuButton" table="v_user_menu_button">
		<id column="menu_button_pk" name="menuButtonPk" type="string" unsaved-value="0">
			<generator class="guid" />
		</id>
		<property column="user_pk" 			name="userPk" 			type="string" />
		<property column="role_pk" 			name="rolePk" 			type="string" />
		<property column="page_id" 		name="pageId" 		type="string" /> 
		<property column="button_id" 		name="buttonId" 		type="string" /> 
	</class>
</hibernate-mapping>